<h1>Top#index</h1>
<p>Find me in app/views/top/index.html.erb</p>

<div>
  <%= form_for(:form, url: sign_in_path) do |f| %>
    <div>
      <p>Email</p>
      <p><%= f.email_field :email, autofocus: true, placeholder: 'Email' %></p>
    </div>
    <div>
      <p>Password</p>
      <p><%= f.password_field :password, autofocus: true, placeholder: 'Password' %></p>
    </div>
    <%= f.submit 'sign in' %>
  <% end %>
</div>
